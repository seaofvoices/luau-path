{
  "name": "test-place",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "node_modules": {
        "$path": "./node_modules"
      },
      "TestTarget": {
        "$path": "./src",
        "jest.config": {
          "$path": "./jest.config.luau"
        }
      }
    },
    "ServerScriptService": {
      "RunTests": {
        "$path": "./scripts/roblox-test.server.luau"
      }
    },
    "HttpService": {
      "$properties": {
        "HttpEnabled": true
      }
    },
    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    }
  }
}
